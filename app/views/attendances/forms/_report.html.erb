<div class="card col-md-10 p-0 offset-md-1">
  <div class="card-header">
    <h3 class = 'text-center'>
  		Adicionar laudo
		</h3>
  </div>
  <div class="card-body">
    <%= form_for(@attendance, {url: add_report_path, method: :patch, multipart: true}) do |f| %>
    	<div class = 'form-group'>
	    	<label>Selecion um arquivo</label>
	    	<%= f.file_field :report, {class: 'form-control-file', required: true} %>
    	</div>
    	<button class = 'btn btn-outline-primary float-right'>
    		Salvar
    	</button>
    <% end %>
  </div>
</div>