<div class="form-group">
  <%= label_tag :patient_id %>
  <%= text_field_tag :patient_id, @attendance.patient.name, {disabled: true, class: 'form-control'}%>
</div>

<div class="form-group">
  <%= label_tag :desease_stage_id %>
  <%= select_tag :desease_stage_id, options_from_collection_for_select(@desease_stages, 'id', 'name'), {class: 'form-control'} %>
</div>
<div class="form-group">
  <%= label_tag :cid_code %>
  <%= text_field_tag :cid_code, '' ,{class: 'form-control'} %>
</div>
<div class="form-group">
  <%= label_tag :lis_code %>
  <%= text_field_tag :lis_code, '', {class: 'form-control'} %>
</div>
<div class="form-group">
  <%= label_tag :health_ensurance_id %>
  <%= select_tag :health_ensurance_id, options_from_collection_for_select(@health_ensurances, 'id', 'name'), {class: 'form-control'} %>
  <% text_field_tag :health_ensurance_id %>
</div>
<div class="form-group">
  <%= label_tag :doctor_name %>
  <%= text_field_tag :doctor_name, '', {class: 'form-control'} %>
</div>
<div class="form-group">
  <%= label_tag :doctor_crm %>
  <%= number_field_tag :doctor_crm, '', {class: 'form-control'} %>
</div>
<div class="form-group">
  <%= label_tag :observations %>
  <%= text_area_tag :observations, '', {class: 'form-control'} %>
</div>  